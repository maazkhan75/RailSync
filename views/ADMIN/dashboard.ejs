<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head.ejs') %>

<body>

<%- include('./partials/header.ejs') %>
<%- include('./partials/sidebar.ejs') %>

<main id="main" class="main">

  <div class="pagetitle">
    <h1>Dashboard</h1>
    <nav>
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="DS">Home</a></li>
        <li class="breadcrumb-item active">Dashboard</li>
      </ol>
    </nav>
  </div>


  <section class="section contact">
    
    <div class="row gy-4">
      <div class="col-xl-6">
        <div class="row">

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link" href="addTrain">
                  <i class="bi bi-train-front"></i>
                  <span>Add Train</span>
                </a>
              </li>
            </div>
          </div>

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link" href="addCarriage">
                  <i class="bi bi-plus-circle"></i>
                  <span>Add Carriage</span>
                </a>
              </li>
            </div>
          </div>

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link collapsed" href="addStation">
                  <i class="bi bi-building-add"></i>
                  <span>Add Station</span>
                </a>
              </li>
            </div>
          </div>

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link collapsed" href="addTrack">
                  <i class="bi bi-node-plus"></i>
                  <span>Add Track</span>
                </a>
              </li>
            </div>
          </div>

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link collapsed" href="addRoute">
                  <i class="bi bi-sign-intersection-fill"></i>
                  <span>Add Route</span>
                </a>
              </li>
            </div>
          </div>

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link collapsed" href="addSecurity">
                  <i class="bi bi-person-fill-add"></i>
                  <span>Add Security</span>
                </a>
              </li>
            </div>
          </div>

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link collapsed" href="addPilot">
                  <i class="bi bi-person-plus"></i>
                  <span>Add Pilot</span>
                </a>
              </li>
            </div>
          </div>

        </div>
      </div>
    </div>

  </section>





</main>


  <%- include('./partials/footer.ejs') %>
  <%- include('./partials/script.ejs') %>

</body>

</html>