<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head.ejs') %>

<body>

<%- include('./partials/header.ejs') %>

<%- include('./partials/sidebar.ejs') %>
<main id="main" class="main">

    <div class="pagetitle">
      <h1>Edit Carriage</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="adminDash">Admin Dashboard</a></li>
          <li class="breadcrumb-item"><a href="trainsData.ejs">Trains Data</a></li>
          <li class="breadcrumb-item active">Edit Carriage</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->
    <section class="section">
      <div class="row">


        

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Carriage Details :</h5>

              <!-- Multi Columns Form -->
              <form class="row g-3">  
              <div class="col-md-6">
                <label for="inputName5" class="form-label" style="color: #de6b00;">CarriageId</label>
                <input type="text" class="form-control inputs" id="inputName5">
            </div>
            
                <div class="col-md-6">
                  <label for="inputName5" class="form-label" style="color: #de6b00;">TrainID</label>
                  <input type="text" class="form-control inputs" id="inputName5">
                </div>
                
                <div class="col-md-6">
                  <label for="inputState" class="form-label">Type</label>
                  <select id="inputState" class="form-select inputs selector" style="color: #de6b00;">
                    <option selected>E</option>
                    <option>F</option>
                    <option>B</option>
                  </select>
                </div>
               
                <div class="col-12 text-left">
                  <button type="submit" id="submitButton" class="btn custom-btn">Submit</button>
                  <button type="reset" class="btn custom-btn-secondary">Reset</button>
                </div>
              </form><!-- End Multi Columns Form -->

            </div>
          </div>


        </div>

    </section>

  </main><!-- End #main -->


  <%- include('./partials/footer.ejs') %>
  <%- include('./partials/script.ejs') %>

</body>

</html>