  </div><!-- End Page Title -->

  <section class="section contact">
    
    <div class="row gy-4">
      <div class="col-xl-12">
        <div class="row">

          <div class="col-lg-6">
            <div class="info-box card">
              <li class="card-item">
                <a class="card-link" href="showTicketsOfUser">
                  <i class="bi bi-card-list"></i>
